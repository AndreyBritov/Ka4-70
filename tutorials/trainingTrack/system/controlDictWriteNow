FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pimpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          writeNow;

endTime         3.5;

deltaT          1e-7;

writeControl     adjustable;//timeStep;

writeInterval   0.15;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           1;//0.5;
libs
(
    "libturbinesFoam.so"
);

functions
{
    wallShearStress1
    {
        type        wallShearStress;
        libs        ("libfieldFunctionObjects.so");
        patches     (down);
        writeControl    writeTime;
    }
    #include "fieldAverage1"
    #include "sample"

}

